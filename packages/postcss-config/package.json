{
  "name": "@wearerequired/postcss-config",
  "version": "0.2.0",
  "description": "Shareable PostCSS config used by @wearerequired.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wearerequired/js.git"
  },
  "keywords": [
    "postcss",
    "postcss-config",
    "css"
  ],
  "author": {
    "name": "required gmbh",
    "email": "info@required.com",
    "url": "https://required.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wearerequired/js/issues"
  },
  "homepage": "https://github.com/wearerequired/js/tree/master/packages/postcss-config#readme",
  "dependencies": {
    "cssnano": "^5.0.8",
    "postcss-import": "^14.0.2",
    "postcss-mixins": "^8.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "postcss-sort-media-queries": "^4.2.1"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "jest",
    "update-fixtures": "postcss --config index.js  --no-map test/integration/fixtures/index.css --output test/integration/expected/index.css && NODE_ENV=production postcss --config index.js  --no-map test/integration/fixtures/index.css --output test/integration/expected/index.minified.css"
  },
  "browserslist": [
    "extends @wearerequired/browserslist-config"
  ],
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "devDependencies": {
    "@wearerequired/browserslist-config": "file:../browserslist-config",
    "jest": "^27.1.0",
    "postcss": "^8.3.6",
    "postcss-cli": "^9.0.2",
    "postcss-load-config": "^3.1.0"
  }
}
